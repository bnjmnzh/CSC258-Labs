module mux2to1(x, y, s, m);
    input x; //selected when s is 0
    input y; //selected when s is 1
    input s; //select signal
    output m; //output
  
    assign m = s & y | ~s & x;
    // OR
    // assign m = s ? y : x;

endmodule

module mux4to1(x, y, s, m);
	input [9:0] SW;
	output [9:0] LEDR;
	wire first, second;
	
	mux2to1 u0(
		.x(SW[0]),
		.y(SW[1]),
		.s(SW[8]),
		.m(first)
	);
	
	mux2to1 u1(
		.x(SW[2]),
		.y(SW[3]),
		.s(SW[8]),
		.m(second)
	);
	
	mux2to1 u2(
		.x(first),
		.y(second),
		.s(SW[9]),
		.m(LEDR[0])
	);
	
endmodule